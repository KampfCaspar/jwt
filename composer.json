{
  "name": "kampfcaspar/jwt",
  "description": "JWT PHP library abstraction layer",
  "homepage": "https://github.com/KampfCaspar/JWT",
  "license": "AGPL-3.0-or-later",
  "type": "library",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "psr/log": "^3.0.0",
    "psr/clock": "^1.0",
    "kampfcaspar/filter": "^0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "struggle-for-php/sfp-phpstan-psr-log": "^0.17",
    "beste/psr-testlogger": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "KampfCaspar\\JWT\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KampfCaspar\\Test\\JWT\\": "tests/"
    }
  },
  "keywords": [
    "jwt",
    "jws",
    "jwe"
  ],
  "authors": [
    {
      "name": "KampfCaspar",
      "email": "code@kampfcaspar.ch",
      "role": "Developer"
    }
  ],
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
